        70% { transform: translate(3px, 1px) rotate(-1deg); }
        80% { transform: translate(-1px, -1px) rotate(1deg); }
        90% { transform: translate(1px, 2px) rotate(0deg); }
        100% { transform: translate(1px, -2px) rotate(-1deg); }
    }


    .container .calling-container .calling-icon div .decline:hover{
        animation:callDecline .4s linear infinite;
    }

    @keyframes callDecline {
        0% { transform: translate(1px, 1px) rotate(140deg); }
        10% { transform: translate(-1px, -2px) rotate(141deg); }
        20% { transform: translate(-3px, 0px) rotate(140deg); }
        30% { transform: translate(3px, 2px) rotate(140deg); }
        40% { transform: translate(1px, -1px) rotate(140deg); }
        50% { transform: translate(-1px, 2px) rotate(139deg); }
        60% { transform: translate(-3px, 1px) rotate(140deg); }
        70% { transform: translate(3px, 1px) rotate(141deg); }
        80% { transform: translate(-1px, -1px) rotate(141deg); }
        90% { transform: translate(1px, 2px) rotate(140deg); }
        100% { transform: translate(1px, -2px) rotate(141deg); }
    }

    .container .calling-container .calling-icon div .decline{
        transform:rotate(140deg);
        background:#DD0200;
    }


    .container .calling-container .calling-icon div .accept{
        background:#04B72F;
    }

